
@model IEnumerable<CM.Models.Candidate>
@using PagedList.Mvc;


<!DOCTYPE html>

<html>
<head>
	<meta name="viewport" content="width=device-width" />
	<title>_List</title>
</head>
<body>
	<p>
		@Html.ActionLink("Create New", "Create")
	</p>

		<table class="table">
			<tr>
					<th>
						@Html.DisplayNameFor(model => model.Account)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.National_id)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.CandidateName)
					</th>
