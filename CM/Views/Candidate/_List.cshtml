						@Html.DisplayFor(modelItem => item.Gender)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Email)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Phone)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Facebook)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.GraduationDate)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.WorkingTimeAvaiable)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Skill)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.GPA)
					</td>
					<td>
						@Html.DisplayFor(modelItem => item.Faculty_of_University.id)
					</td>
				
				</tr>
			}
	}
		</table>
	
