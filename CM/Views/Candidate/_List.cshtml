						@Html.DisplayNameFor(model => model.Faculty_of_University.id)
					</th>
					<th></th>
				
				</tr>
	@if(Model!=null)
	{
			foreach (var item in Model)
			{
				<tr>
					<td>
						@Html.DisplayFor(modelItem => item.Account)
